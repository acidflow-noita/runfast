    <item>
      <title>Random Spell</title>
      <link>https://runfast.stream/twitch/twitch-integration/random-spell/</link>
      <pubDate>Mon, 01 Jul 2024 15:37:25 +1200</pubDate>
      
      <guid>https://runfast.stream/twitch/twitch-integration/random-spell/</guid>
